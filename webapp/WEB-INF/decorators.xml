<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE decorators>
<decorators defaultdir="/WEB-INF/jsp/decorators">
	<excludes>
		<pattern>/ajax/*</pattern>
		<pattern>/api/*</pattern>
		<pattern>/popup/*</pattern>
		<pattern>/blank</pattern>
		<pattern>/attachments*</pattern>
		<pattern>/rss*</pattern>
		<pattern>/wikis/preview*</pattern>
		<pattern>/robots.txt</pattern>
		<pattern>/google528d99e4fd7e1630.html</pattern>
	</excludes>

	<decorator name="admin" page="admin_default.jsp">
		<pattern>/admin/*</pattern>
	</decorator>
	
	<decorator name="default" page="default.jsp">
		<pattern>/*</pattern>
	</decorator>
</decorators>